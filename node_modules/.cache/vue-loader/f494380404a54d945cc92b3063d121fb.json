osition: relative;\n\t\t&.light {\n\t\t\tbackground: #fff;\n\t\t}\n\t\t&.dark {\n\t\t\tbackground: #001529;\n\t\t}\n\t\t.global-header-wide {\n\t\t\t&.head {\n\t\t\t\tmax-width: 1400px;\n\t\t\t\tmargin: auto;\n\t\t\t}\n\t\t\t&.side {\n\t\t\t}\n\t\t\t.logo {\n\t\t\t\theight: 64px;\n\t\t\t\tline-height: 58px;\n\t\t\t\tvertical-align: top;\n\t\t\t\tdisplay: inline-block;\n\t\t\t\tpadding: 0 12px 0 24px;\n\t\t\t\tcursor: pointer;\n\t\t\t\tfont-size: 20px;\n\t\t\t\t&.pc {\n\t\t\t\t\tpadding: 0 12px 0 0;\n\t\t\t\t}\n\t\t\t\timg {\n\t\t\t\t\tdisplay: inline-block;\n\t\t\t\t\tvertical-align: middle;\n\t\t\t\t}\n\t\t\t\th1 {\n\t\t\t\t\tdisplay: inline-block;\n\t\t\t\t\tfont-size: 16px;\n\t\t\t\t}\n\t\t\t\t&.dark h1 {\n\t\t\t\t\tcolor: #fff;\n\t\t\t\t}\n\t\t\t}\n\t\t\t.global-header-menu {\n\t\t\t\tdisplay: inline-block;\n\t\t\t}\n\t\t\t.global-header-right{\n\t\t\t\tfloat: right;\n\t\t\t\t&.dark {\n\t\t\t\t\tcolor: #fff;\n\t\t\t\t\ti {\n\t\t\t\t\t\tcolor: #fff;\n\t\t\t\t\t}\n\t\t\t\t}\n\t\t\t}\n\t\t}\n\t}\n</style>\n"]}]}